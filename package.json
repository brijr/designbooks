    "@payloadcms/storage-vercel-blob": "^3.4.0",
    "@tailwindcss/typography": "^0.5.15",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "libsql": "0.5.0-pre.2",
    "next": "15.0.3",
    "payload": "latest",
    "react": "19.0.0-rc-66855b96-20241106",
